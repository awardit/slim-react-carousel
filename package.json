  "repository": {
    "type": "git",
    "url": "git@github.com:peterasplund/slim-react-carousel.git"
  },
  "scripts": {
    "dev": "rollup -w --config rollup.config.example.js",
    "build": "rollup --config rollup.config.prod.js",
